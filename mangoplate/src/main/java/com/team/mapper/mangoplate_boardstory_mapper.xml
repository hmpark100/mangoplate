<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="mapper.boardstory">
	<!-- board insert  -->
	<insert id="insert" parameterType="com.mangoplate.vo.MangoBoardStoryVO" >
		insert into mg_board_story
				 values('b_'||sequ_mg_board_story.nextval, #{sname}, #{editer}, #{stitle}, #{sstitle}, 
				 sysdate, 0, #{scontent}, #{simage1}, #{ssimage1}, #{simage2}, #{ssimage2})
	</insert>
</mapper>    
    